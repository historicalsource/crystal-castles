CL.TBB:
.RADIX 2
	.BYTE 00110110
;  1-4
	.BYTE 10011000
	.BYTE 10011000
	.BYTE 10111000
	.BYTE 10011000
;  5-8
	.BYTE 10101100
	.BYTE 11011000
	.BYTE 10011100
	.BYTE 10011000
